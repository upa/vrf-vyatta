type: u32
help: Summary-default cost of nssa area
syntax:expression: $VAR(@) >= 0 && $VAR(@) <= 16777215; "Cost must be between 0-16777215"
val_help: u32:0-16777215; Summary default cost

create: sudo /usr/local/bin/vtysh -x /var/run/quagga-$VAR(../../../../../../@) \
	  -c "configure terminal" \
          -c "router ospf"                                        \
          -c "area $VAR(../../../@) nssa"                         \
          -c "area $VAR(../../../@) default-cost $VAR(@)";

update: sudo /usr/local/bin/vtysh -x /var/run/quagga-$VAR(../../../../../../@) \
	  -c "configure terminal" \
          -c "router ospf"                                        \
          -c "area $VAR(../../../@) default-cost $VAR(@)"; 

delete: sudo /usr/local/bin/vtysh -x /var/run/quagga-$VAR(../../../../../../@) \
	  -c "configure terminal" \
          -c "router ospf"                                        \
          -c "no area $VAR(../../../@) default-cost $VAR(@)";
