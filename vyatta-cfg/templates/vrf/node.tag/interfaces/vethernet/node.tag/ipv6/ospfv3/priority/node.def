type: u32
help: Router priority
default: 1
syntax:expression: $VAR(@) >= 0 && $VAR(@) <= 255; "Must be between 0-255"
val_help: u32:0-255; Priority (default 1)

update: sudo /usr/local/bin/vtysh -x /var/run/quagga-$VAR(../../../../../@) \
	-c "configure terminal" -c "interface $VAR(../../../@)" -c "ipv6 ospf6 priority $VAR(@)"
delete: sudo /usr/local/bin/vtysh -x /var/run/quagga-$VAR(../../../../../@) \
	-c "configure terminal" -c "interface $VAR(../../../@)" -c "ipv6 ospf6 priority 1"
